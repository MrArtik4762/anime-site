{
  "name": "anime-site-server",
  "version": "1.0.0",
  "description": "Backend API для аниме-сайта",
  "main": "server.js",
  "scripts": {
    "start": "node -r module-alias/register server.js",
    "dev": "nodemon -r module-alias/register server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=jest.integration.config.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "axios": "^1.3.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "express-winston": "^4.2.0",
    "helmet": "^6.1.5",
    "helmet-csp": "^4.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.5.1",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "pg": "^8.11.0",
    "prom-client": "^14.2.0",
    "qrcode": "^1.5.4",
    "redis": "^4.6.5",
    "socket.io": "^4.6.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-elasticsearch": "^0.17.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "express",
    "mongodb",
    "api",
    "anime",
    "backend"
  ],
  "author": "Anime Site Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "_moduleAliases": {
    "/app": ".."
  }
}
